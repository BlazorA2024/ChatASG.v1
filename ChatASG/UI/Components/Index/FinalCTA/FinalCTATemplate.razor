    <!-- Final CTA -->
@using global::Data.FinalCTA
@using global::Data.FinalCTAModul
<AddCardFinalCTA Prams="Prams" />

@code {

    private DataFinalCTA FinalCTA = new()
    {
      InputPlaceholder = "Enter your email",
       Button = "Get Started",
    };

    [Parameter]
    public DataAddFinalCTA? DataBuild { get; set; }

    private CardAddFinalCTA Prams = new();

    protected override void OnInitialized()
    {
        if (DataBuild == null)
        {
            DataBuild = new DataAddFinalCTA
                {
                    Title = "Ready to Transform How You Work?",
                    Description = "Join thousands of professionals who have boosted their productivity with AI. Sign up today and experience the difference.",
                    //InputPlaceholder = "Enter your email",
                   // ButtonText = "Get Started",
                    IFinalCTA = FinalCTA,
                    Features = new()
                            {
                                "Free 14-day trial",
                                "No credit card required"
                            }
                };
        }

        Prams = CardAddFinalCTA.Create(DataBuild);
        base.OnInitialized();
    }
}
